<div id="header">
  <% if auth_user_info %>
    <p>Welcome, <%= auth_user_info["given_name"] %></p>
    <img id="profile-photo" src="<%= auth_user_info["picture"] %>">
    <a href="/users/logout">Logout</a>
  <% else %>
    <img id="front-icon" src="/img/front-icon.png">
    <div id="login-button">
      <a id="login-link" href="<%= auth_authorize_link %>">Login</a>
    </div>
  <% end %>
</div>
